<% -include("./partials/header")%>
<main>

    <body>
        <div class="nossoSocialMidia">
        
            <a><img src="/public/img/facebook.png" alt="Ícone clicável para a nossa página do facebook"></a>
            <a><img src="/public/img/google.png" alt="Ícone clicável para a nossa página do google"></a>
            <a><img src="/public/img/instagram.png" alt="Ícone clicável para a nossa página do instagram"></a>
            <a><img src="/public/img/twitter.png" alt="Ícone clicável para a nossa página do twitter"></a>
        
        </div>

        <h3 id="tituloCadastroCliente">Acesse a sua área</h3>


        <div class="loginCadastro">


            <div class="loginSocialMidia">
                <p>
                <h4>Não é cadastrado? Entre com:</h4>
                </p>

                <div class="iconesLogin">

                    <a rel="noreferrer noopener" target="_blank" href="https://pt-br.facebook.com/">
                        <img class="socialmidia" src="/public/img/facebook.png" alt="Ícone para logar pelo facebook">
                    </a>

                    <a rel="noreferrer noopener" target="_blank" href="https://www.instagram.com/accounts/login/">
                        <img class="socialmidia" src="/public/img/instagram.png" alt="Ícone para logar pelo instagram">
                    </a>

                    <a rel="noreferrer noopener" target="_blank"
                        href="https://twitter.com/i/flow/login?input_flow_data=%7B%22requested_variant%22%3A%22eyJsYW5nIjoicHQifQ%3D%3D%22%7D">
                        <img class="socialmidia" src="/public/img/twitter.png" alt="Ícone para logar pelo twitter">
                    </a>

                    <a rel="noreferrer noopener" target="_blank"
                        href="https://accounts.google.com/signin/v2/identifier?hl=pt-br&flowName=GlifWebSignIn&flowEntry=ServiceLogin">
                        <img class="socialmidia" src="/public/img/google.png" alt="Ícone para logar pelo google">
                    </a>
                </div>

                <div class="login">
                    <p>
                    <h4>Entre com a sua conta</h4>
                    </p>
                    <input type="email" title="email" value="Seu e-mail" />
                    <input type="password" title="senha" value="Digite a sua senha" />
                    <input type="submit" id="botaoEntrar" value="Entrar" />
                </div>

            </div>

        </div>

        <h4>Mais procurados...</h4>


      <% -include("./partials/footer")%>
        